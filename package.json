  "prettier": {
    "semi": false,
    "useTabs": true,
    "printWidth": 80,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "devDependencies": {
    "@types/blueimp-load-image": "^5.14.4",
